        		<!-- SLOGAN --> 
                <div id="slogan" class="inner">
                    <h1 class="only"><%= header %></h1>
                    <div><a href="stud_logout">logout</a></div>
                </div>         
                <!-- END SLOGAN --> 
                
                <!-- HOME SECTIONS -->   
                <div class="home-sections">
                    <form name="connect" action="/notifySponsor" method="post">
                    	<div class="toggle">
                        	<p class="tab-index tab-opened"><a href="#" title="Close" class=" opened"><b>Individual Sponsors</b></a></p>
            				<div class="content-tab opened">
				     			<table cellpadding="0" cellspacing="0" class="short-table blue toggled">
				            		<thead>
				        				<tr>                
				            				<th style="width:5%">Select</th> 
				            				<th style="width:15%">Name</th>                           
				            				<th style="width:15%">email</th>              
				            				<th style="width:15%">SponsorShip Type</th>
				            				<th style="width:35%">Criteria Specified by Sponsors</th>
				        				</tr>
				 					</thead>     
				 					<tbody>
				     					<% results.forEach(function(sponsor){ %>
				     						<% if(sponsor.type=='individual'){ %>
				    							<tr>
				      								<td><input type="checkbox" name="sponsorList" id="sponsorList" value='<%=sponsor.sid%>'></input></td>
				     								<td><%=sponsor.name%></td>
				     								<td><%=sponsor.email%></td>
				     								<td><%=sponsor.stype%></td>
				     								<td><textarea readonly row="200" cols="50" id="criteria" name="criteria"><%=sponsor.criteria%></textarea></td>
				     							</tr>
				     						<% } %>     
				     					<% }) %>
									</tbody>
				      			</table>
            				</div>	
            			</div>   
            			<div class="toggle">
                        	<p class="tab-index tab-opened"><a href="#" title="Close" class=" opened"><b>Organization Sponsors</b></a></p>
            				<div class="content-tab opened">
				     			<table cellpadding="0" cellspacing="0" class="short-table blue toggled">
			 					<thead>
			        				<tr>                
			            				<th style="width:5%">Select</th> 
			            				<th style="width:15%">Name</th>                           
			            				<th style="width:15%">email</th>              
			            				<th style="width:15%">SponsorShip Type</th>
			            				<th style="width:35%">Criteria Specified by Sponsors</th>
			        				</tr>
			 					</thead>     
			 					<tbody>  
									<% results.forEach(function(sponsor){ %>
			 							<% if(sponsor.type=='organization'){ %>
											<tr>
			  									<td><input type="checkbox" name="sponsorList" id="sponsorList" value='<%=sponsor.sid%>'></input></td>
			 									<td><%=sponsor.name%></td>
			 									<td><%=sponsor.email%></td>
			 									<td><%=sponsor.stype%></td>
			 									<td><textarea readonly row="200" cols="50" id="criteria" name="criteria"><%=sponsor.criteria%></textarea></td>
			 								</tr>
			 							<% } %>     
			 						<% }) %>  
			 					</tbody>
			  				</table>
            				</div>	
            			</div>
            				
				    	<input type="submit" value="submit" class="large blue button"></input>
					</form>
