        <link rel="stylesheet" id="prettyPhoto-css" href="stylesheets/prettyPhoto.css" type="text/css" media="all" />
        <link rel="stylesheet" id="jquery-tipsy-css" href="stylesheets/tipsy.css" type="text/css" media="all" />
        
        <script type="text/javascript" src="js/jquery.js"></script>
        <script type="text/javascript" src="js/jquery.easing.1.3.js"></script>
        <script type="text/javascript" src="js/jquery.prettyPhoto.js"></script>
        <script type="text/javascript" src="js/jquery.tipsy.js"></script>
        <script type="text/javascript" src="js/jquery.tweetable.js"></script>        
        <script type="text/javascript" src="js/jquery.nivo.slider.pack.js"></script>  
        <script type="text/javascript" src="js/jquery.flexslider.min.js"></script>
        <script type="text/javascript" src="js/jquery.cycle.min.js"></script>  
        
        <!-- for accordion slider in staff page -->
        <script type="text/javascript" src="js/jquery.hrzAccordion.js"></script>  
        
        <!-- for filterable effect in gallery and portfolio filterable page -->
        <script type="text/javascript" src="js/jquery.quicksand.js"></script>  
        
        <!-- for portfolio slider -->
        <script type="text/javascript" src="js/jquery.jcarousel.min.js"></script> 
        
        <!-- for the contact page -->
        <script type="text/javascript" src="js/contact.js"></script>  
        
        <!-- SLIDER ELASTIC -->                                                               
        <link rel="stylesheet" id="slider-elastic-css" href="stylesheets/slider-elastic.css" type="text/css" media="all" />
        <script type="text/javascript" src="js/jquery.eislideshow.js"></script>
        
        <!-- SLIDER CYCLE -->                                                               
        <link rel="stylesheet" id="slider-cycle-css" href="stylesheets/slider-cycle.css" type="text/css" media="all" />
        <script type="text/javascript" src="js/jquery.slides.min.js"></script>
        
        <!-- SLIDER THUMBNAILS -->                                                               
        <link rel="stylesheet" id="slider-thumbnails-css" href="stylesheets/slider-thumbnails.css" type="text/css" media="all" />
        <script type="text/javascript" src="js/jquery.aw-showcase.js"></script>
        
        <!-- SLIDER FLASH -->                                                               
        <script type="text/javascript" src="js/swfobject.js"></script>      
        
        <!-- SLIDER ELEGANT -->                                                               
        <link rel="stylesheet" id="slider-elegant-css" href="stylesheets/slider-elegant.css" type="text/css" media="all" />
        
        <!-- SLIDER NIVO -->                                                               
        <link rel="stylesheet" id="slider-nivo-css" href="stylesheets/slider-nivo.css" type="text/css" media="all" />     
        
        <!-- CUSTOM JAVASCRIPT -->                           
        <script type="text/javascript" src="js/jquery.custom.js"></script>
        
        <script type="text/javascript">
	        function getPreviousReviews(){
				var pageNumber=$("#pageNumber").val();
	         	pageNumber=parseInt(pageNumber)-1;
	         	var departmentUrl= "<%=department%>";
	         	var universityUrl= "<%=school%>";
	       
	         	$.ajax({
			         type:'GET',
			         url:'/getMoreProfessorReview?department='+departmentUrl+'&institution='+universityUrl+'&page='+pageNumber,
			         success:function(data){
			       		 $("#primary").html(data);
			         }
	         	});	        
	        } 
        </script>
        <script type="text/javascript">
			function getMoreReviews(){
				var pageNumber=$("#pageNumber").val();
     			pageNumber=parseInt(pageNumber)+1;
     			var departmentUrl= "<%=department%>";
     			var universityUrl= "<%=school%>";
 
				$.ajax({
     				type:'GET',
     				url:'/getMoreProfessorReview?department='+departmentUrl+'&institution='+universityUrl+'&page='+pageNumber,
     				success:function(data){
   						$("#primary").html(data);
     				}
     			});   
	        }
        </script>
    </head>
    
    <body class="image-sphere-style responsive">
    
        <!-- START SHADOW WRAPPER -->
        <div class="shadowBg group">
            
            <!-- START WRAPPER -->
            <div class="wrapper group">
                 
                <!-- START HEADER -->
                <div id="header" class="group">
                
                    <!-- START LOGO -->
                    <div id="logo" class="group">
                        <a href="home" title="Fly US"> 
                        <img src="images/edu.jpeg" width = "200" height = "60" alt="EducationUSA" />
                        </a>              
                    </div>
                    <!-- END LOGO -->
                    
                    <!-- START NAV -->
                    <div id="nav" class="group">
                        <ul class="level-1 white">
                            <li class="home">
                                <a href="home">HOME</a>
                            </li>
                            <li class="folder">
                                <a href="gradSchool">Grad School</a>
                            </li>
                            <li class="people">
                                <a href="profiler">Profiler</a>
                            </li>
                            <li class="pen">
                                <a href="professorReviews">Professor Reviews</a>
                            </li>
                            <li class="burst">
                                <a href="finance">Finance</a>
                                <ul class="sub-menu">
                                    <li><a href="scholarship">ScholarShip</a></li>
	                                <li><a href="student">Connect To Sponsor</a></li>
	                                <li><a href="Sponsor">Become a Sponsor</a></li>
                                </ul>
                            </li>
                            <li class="burst">
                                <a href="immigration">Immigration</a>
                                <ul class="sub-menu">
                                    <li><a href="visaStatsInd">Visa Stats</a></li>
	                                <li><a href="viQuestions">Sample Visa Questions</a></li>
	                                <li><a href="ds160faqs">Visa Types</a></li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                    <!-- END NAV -->
                </div>
                <!-- END HEADER -->        
        
                <!-- START CONTENT -->
                <div id="content" class="layout-sidebar-no group">
                
                    <!-- SLOGAN --> 
                    <div id="slogan" class="inner">
                        <h1 class="only">Suggested Universities</h1>
                    </div>         
                    <!-- END SLOGAN --> 
                
                    <!-- START PRIMARY -->
                    <div id="primary" class="hentry group wrapper-content" role="main">
						<!-- START TABLE -->
						<% var result_page = title %>
                    	<% if(result_page == 'University'){ %>
	                        <table style="width:80%" cellpadding="0" cellspacing="0" class="short-table blue">
	                            <thead>
	                                <tr>
	                                    <th style="width:20%">S.No</th>               
	                                    <th style="width:20%">Name</th>             
	                                    <th style="width:20%">URL</th>              
	                                </tr>
	                            </thead>
	                    
	                            <tbody>
	                            	<% var i = 1 %>
	                            	<%results.forEach(function(college){%>
		                                <tr>           
		                                    <td><%= i++ %></td>                                       
		                                    <td><%= college.University_Name%></td>                                
		                                    <td><%= college.Website%></td>
		                                </tr>
	                                <% }) %>
	                            </tbody>
	                        </table>
	                        <div id='next' class='button' onclick="callNext();">next</div>
                        <% } else { %>
                        	<input type="hidden" id="pageNumber" value="<%=pageNumber%>"/>
                        	<table style="width:80%" cellpadding="0" cellspacing="0" class="short-table blue">
	                            <thead>
	                                <tr>
	                                    <th style="width:20%">S.No</th>               
	                                    <th style="width:20%">Professor Name</th>             
	                                    <th style="width:20%">Rating</th>              
	                                    <th style="width:20%">Overall Rating(Out of 5)</th>
	                                </tr>
	                            </thead>
	                            <tbody>
	                            	<% var i = 1 %>
	                            	<%results.forEach(function(professor){%>
		                                <tr>           
		                                    <td><%= i++ %></td>                                       
		                                    <td><%=professor.tFname + " " + professor.tLname%></td>                                
		                                    <td><%=professor.rating_class%></td>              
		                                    <td><%=professor.overall_rating%></td>
		                                </tr>
	                                <% }) %>
	                            </tbody>
	                        </table>
	                        <% if(pageNumber > 1){ %>
								<input type="button" class="paging" onClick="return getPreviousReviews();" value="Previous"></input>
							<% } %>
 							<% if(nextFlag){ %>
	                        	<input type="button" class="paging" onClick="return getMoreReviews();" value="Next"></input>
	                        <% } %>
                        <% } %>
                        <!-- END TABLE -->
                </div>
                <!-- END CONTENT -->        

                <!-- START TWITTER -->
                <div id="twitter-slider" class="group">
                    <div class="tweets-list"></div>
                    <div class="bird"></div>
                </div>
                <!-- END TWITTER -->   
                
                           
                <!-- START COPYRIGHT -->
                <div id="copyright" class="group two-columns">
                    <div class="inner group">
                        <p class="left">Copyright <a href="#"><strong>Team 16</strong></a> 2014 - Powered by <strong>Dragons</strong></a></p>
                        <p class="right">
                            <a href="#" class="socials facebook" title="Facebook">facebook</a>
                            <a href="#" class="socials twitter" title="Twitter">twitter</a>
                            <a href="#" class="socials linkedin" title="Linkedin">linkedin</a>
                        </p>
                    </div>
                    
                </div>
                <!-- END COPYRIGHT -->  
                   
            </div>
            <!-- END WRAPPER --> 
            	    
        </div>
        <!-- END SHADOW -->       
        
    </body>
</html>