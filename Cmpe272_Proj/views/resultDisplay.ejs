	                                    <th style="width:20%">Rating</th>              
	                                    <th style="width:20%">Overall Rating(Out of 5)</th>
	                                </tr>
	                            </thead>
	                    
	                            <tbody>
	                            	<% var i = 1 %>
	                            	<%results.forEach(function(professor){%>
		                                <tr>           
		                                    <td><%= i++ %></td>                                       
		                                    <td><%=professor.tFname + " " + professor.tLname%></td>                                
		                                    <td><%=professor.rating_class%></td>              
		                                    <td><%=professor.overall_rating%></td>
		                                </tr>
	                                <% }) %>
