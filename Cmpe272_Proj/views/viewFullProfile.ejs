        <h1>Become a Sponsor</h1>
    </head>
    
    <body>
    <form name="profileForm" action="/changeStatus" method="post">
    
    <h1><align="center"><%=results.name%></align></h1>
    <input type="hidden" value='<%=results.sid%>' name="studentId" id="studentId"></input>
     <table>
    <tr>
    <td>Name</td>
    <td><%=results.name%></td>
    </tr>
    <tr>
    <td>Country</td>
    <td><%=results.country%></td>
    </tr>
   <tr>
   <td>Schools:</td>
   </tr>
   <tr>
   
   <%students.forEach(function(student){%>
   <tr>
   <td><%=student.school%></td>
   <td><%=student.course%></td>
   <td><%=student.fees%></td>
   
  </tr>
  <%})%>
   </tr>
    <tr>
    <td>Email</td>
    <td><%=results.email%></td>
    </tr>
    <tr>
    <td>resume</td>
    <Td><a href='<%=results.resume%>' download ><%=results.name%> Resume</a></td>
    </tr>
    
    <tr>
    <td>Justification</td>
    <Td><a href='<%=results.justification%>' download><%=results.name%> Reason</a></td>
    </tr>
    <tr>
    <td>Change Status</td>
    <td>
    <select id="status" value="status" name="status">
    <option value="under review">Under Review </option>
    <option value="accepted" > Accepted </option>
    <option value="Rejected">Rejected </option>
    </select>
    </td>
    </tr>
    <tr>
    <td>Comment To Be Provided </td>
    <td><textarea rows="6" columns="50" id="comment" name="comment"></textarea></td>
    </tr>
    
    <tr>
    <td><input type="submit" value="submit"></input></td>
    </tr>
    </table>
    
    </table>
    
    </body>
